# ============================================
# MedAgentBench Docker Environment Variables
# ============================================
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# --------------------------------------------
# Required: Google API Key
# --------------------------------------------
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# --------------------------------------------
# Green Agent Configuration
# --------------------------------------------
# Path to test data JSON file (inside container)
DATA_PATH=data/medagentbench/test_data_v2.json

# Enable official MedAgentBench format export (outputs/[model]/[task]/overall.json)
EXPORT_OFFICIAL=true

# Output directory for results
OUTPUT_DIR=outputs

# Model name for output directory naming
MODEL_NAME=agentx-medical

# --------------------------------------------
# Purple Agent Configuration
# --------------------------------------------
# Medical specialty: diabetes, cardiology, internal_medicine, general_medical
MEDICAL_SPECIALTY=diabetes

# --------------------------------------------
# FHIR Server Configuration (Optional)
# --------------------------------------------
# Only needed if using FHIR server for EHR data queries
# FHIR_BASE_URL=http://fhir-server:8080/fhir

# --------------------------------------------
# Development Settings
# --------------------------------------------
# Set to true for verbose logging
# DEBUG=false

# --------------------------------------------
# Docker Compose Overrides
# --------------------------------------------
# Use different host ports if defaults conflict
# GREEN_AGENT_PORT=9008
# PURPLE_AGENT_PORT=9010
# FHIR_SERVER_PORT=8080
